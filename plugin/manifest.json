{
    "manifest_version": 2,
    "name": "Cantonese Romanizer",
    "description": "Converts Chinese characters in a webpage to Jyutping at the press of a button.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "jyut6.png"
    },
    "background": {
        "scripts": [ "jquery.min.js", "charmap.js", "background.js" ]
    },
    "permissions": [
        "tabs", "*://*/*"
    ]
}